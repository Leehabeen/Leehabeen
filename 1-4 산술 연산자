void main() {

/*산술 연산자
 +  (더하기)
 -  (빼기)
 *  (곱하기)
 /  (나누기)
 ~/ (나눈 후 몫)
 %  (나머지)
 
   */
  
  assert(2 + 3 == 5);
  assert(5 - 2 == 3);
  assert(3 * 5 == 15);
  assert(5 / 2 == 2);
  assert(5 ~/ 2 == 2);
  assert(5 % 2 == 1);
  //assert() 함수는 계산 결과가 참인지 거짓인지 검사해줍니다
  //assert() 함수는 dartpad등의 웹에서는 함수가 동작하지 않습니다
  
  assert('hello' + 'world' == 'hello world');
  //더하기 연산자의 또 다른 쓰임새는 두개의 문자열들을 결합하는데 사용할 수 있습니다
  }

